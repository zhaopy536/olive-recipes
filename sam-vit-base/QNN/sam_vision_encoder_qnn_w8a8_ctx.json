{
    "input_model": {
        "type": "PyTorchModel",
        "model_path": "facebook/sam-vit-base",
        "model_loader": "sam_ve_load",
        "model_script": "user_script.py",
        "io_config": {
            "input_names": [
                "pixel_values"
            ],
            "output_names": [
                "last_hidden_state"
            ]
        },
        "dummy_inputs_func": "vision_encoder_inputs"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "device": "gpu",
                    "execution_providers": [
                        "CUDAExecutionProvider"
                    ]
                }
            ]
        },
        "target_system": {
            "type": "PythonEnvironment",
            "python_environment_path": "/path/to/qnn/env/bin",
            "accelerators": [
                {
                    "execution_providers": [
                        "QNNExecutionProvider"
                    ]
                }
            ]
        }
    },
    "data_configs": [
        {
            "name": "latency_data_config",
            "user_script": "user_script.py",
            "load_dataset_config": {
                "type": "local_dataset"
            },
            "dataloader_config": {
                "type": "vision_encoder_data_loader",
                "batch_size": 1
            }
        },
        {
            "name": "quantize_data_config",
            "user_script": "user_script.py",
            "load_dataset_config": {
                "type": "local_dataset"
            },
            "dataloader_config": {
                "type": "vision_encoder_quantize_data_loader",
                "data_num": 100
            }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "data_config": "latency_data_config",
                    "sub_types": [
                        {
                            "name": "avg",
                            "priority": 1
                        }
                    ]
                }
            ]
        }
    },
    "passes": {
        "convert": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 20,
                "optimize": true,
                "dynamic": false
            }
        },
        "quantization": {
            "type": "OnnxStaticQuantization",
            "data_config": "quantize_data_config",
            "activation_type": "uint8",
            "precision": "uint8",
            "calibrate_method": "MinMax",
            "quant_preprocess": true,
            "calibration_providers": [
                "CUDAExecutionProvider"
            ]
        },
        "cb": {
            "type": "EPContextBinaryGenerator",
            "provider_options": {
                "htp_performance_mode": "burst",
                "htp_graph_finalization_optimization_mode": "3",
                "offload_graph_io_quantization": "0",
                "soc_model": "60"
            },
            "weight_sharing": false
        }
    },
    "log_severity_level": 0,
    "evaluate_input_model": false,
    "host": "local_system",
    "target": "target_system",
    "cache_dir": "cache_olive_ve_w8a8_q100",
    "output_dir": "model/sam_olive_ve_w8a8_q100"
}
