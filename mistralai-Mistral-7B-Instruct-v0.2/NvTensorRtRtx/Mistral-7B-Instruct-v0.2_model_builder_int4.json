{
    "input_model": {
        "type": "HfModel",
        "model_path": "mistralai/Mistral-7B-Instruct-v0.2",
        "task": "text-classification"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "gpu", "execution_providers": [ "NvTensorRTRTXExecutionProvider" ] } ]
